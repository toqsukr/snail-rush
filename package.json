{
  "name": "snail-rush",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:host": "vite --host --mode devhost",
    "build": "tsc && vite build",
    "build:tailscale": "tsc && vite build --mode tailscale",
    "start": "yarn build && vite preview",
    "start:tailscale": "yarn build:tailscale && vite preview --host",
    "lint": "eslint ./src",
    "type-check": "tsc --noEmit",
    "test": "yarn type-check && yarn lint && yarn test:vitest",
    "test:vitest": "vitest",
    "test:coverage": "vitest --coverage"
  },
