    "@tanstack/react-query": "^5.62.14",
    "@vitejs/plugin-basic-ssl": "^2.1.0",
    "@vitejs/plugin-react": "^5.1.0",
    "axios": "^1.7.9",
    "clsx": "^2.1.1",
    "i18next": "^25.6.0",
    "lodash.debounce": "^4.0.8",
    "lodash.throttle": "^4.1.1",
